{
  "manifest_version": 2,
  "name": "Movie Search List",
  "description": "Movie Search List for chrome extension",
  "version": "1.0.0",
  "browser_action": {
    "default_title": "Popup",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "bookmarks", "storage", "identity"],
  "icons": {
    "16": "movieIcon.png",
    "48": "movieIcon.png",
    "128": "movieIcon.png"
  },
  "background": {
    "page": "background.html"
  }
}
